	</replace-with>
    
    <replace-with
        class="com.wfairclough.foundation4gwt.client.ui.resources.internal.DefaultInternalInjector">
        <when-type-is class="com.wfairclough.foundation4gwt.client.ui.resources.internal.InternalResourceInjector"/>
    </replace-with>
